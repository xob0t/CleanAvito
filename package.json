{
  "name": "ave-blacklist",
  "nameFull": "AVE Blacklist",
  "version": "1.0.0",
  "description": "Block unwanted sellers and listings on Avito",
  "main": "ave_blacklist.user.js",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w"
  },
  "author": "xob0t",
  "license": "MIT",
  "homepage": "https://github.com/user/ave-blacklist",
  "bugs": {
    "url": "https://github.com/user/ave-blacklist/issues"
  },
  "userScript": {
    "namespace": "https://github.com/user/ave-blacklist",
    "match": [
      "*://www.avito.ru/*"
    ],
    "run-at": "document-start",
    "grant": [
      "GM_registerMenuCommand",
      "GM_addStyle"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/pluginutils": "^5.1.0",
    "rollup": "^4.9.6",
    "rollup-plugin-banner2": "^1.2.2"
  }
}
